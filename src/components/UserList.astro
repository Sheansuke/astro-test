---
import type { IUser } from "@interfaces/IUser";

const userList: IUser[] = await fetch("https://jsonplaceholder.typicode.com/users").then((res) => res.json());
---

<div>
  <ul>
    {
      userList.map((user) => (
        <li class="p-1">
          <h2 class="font-bold">{user.name}</h2>
          <p> * {user.email}</p>
        </li>
      ))
    }
  </ul>
</div>
